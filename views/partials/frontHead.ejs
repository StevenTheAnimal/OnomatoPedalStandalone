<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    
    <% if ((nav === 'v2') || (nav == 'v2-vbg')) { %>
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <%} else if (nav === 'v1') {%>
        <meta name="viewport" content="width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0">
    <%} else {%>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <% } %>

    <!--<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.6.0/jquery.min.js"></script>-->
    <script>if (typeof module === 'object') {window.module = module; module = undefined;}</script>
    <script src='../js/jquery.min.js'></script>
    <script>if (window.module) module = window.module;</script>
    <link rel="stylesheet" href="../css/style.css">
    <link rel="stylesheet" href="../css/front.css">
    <link rel="stylesheet" href="../css/frontplayer.css">
    <link rel="stylesheet" type="text/css" href="../css/v1style.css" />

    <link rel="icon"  type="image/png"  href="../images/OP_AppIcon.png" />
    <link rel="apple-touch-icon" sizes="128x128" href="../images/OP_AppIcon_128.png" />
    <link rel="icon" sizes="192x192" href="../images/OP_AppIcon_192.png" />

    <% if ( nav === 'v2' || nav == 'v2-vbg') { %>
        <title>OnomatoPedal おのまとペダル - <%= name %> </title>
        <!--<meta property="og:title" content="OnomatoPedal おのまとペダル - <%= name %>" />
        <meta property="og:url" content="https://onomatopedal.com/v2/<%=name%>/" />
        <meta property="og:image" content="https://dev.onomatopedal.com/images/v2cover/og/<%= name %>.jpg" />
        <meta property="og:description" name="og:description" content="OnomatoPedal おのまとペダル - <%= name %> is an Earthquaker Devices inspired online interactive music experience created by <%= pedal.artist %>.">
        <meta property="description" name="description" content="OnomatoPedal おのまとペダル|<%= name %> is an Earthquaker Devices inspired online interactive music experience created by <%= pedal.artist %>.">-->

    <%  } else if (nav === 'v1') { %>
        <title id="pedalPageTitle">OnomatoPedal おのまとペダル | <%= thepedal.onomoname %></title>
        <script type="text/javascript" src="../js/paper-full_0.12.3.js"></script>
        <script type="text/javascript" src="../js/slick.min.js"></script>
    <% } else {%>
        <title>OnomatoPedal | <%= title %> </title>
    <%}%>

    <% if ((nav === 'v2') || (nav == 'v2-vbg')) { %>
        <link rel="stylesheet" href="../css/mobileplayer.css">
    <%}%>

    <!--<script src="https://cdnjs.cloudflare.com/ajax/libs/tone/14.7.77/Tone.js"></script>-->
    <script type="text/javascript" src="../js/Tone.js"></script>
    <meta property="og:type" content="music.song" />
</head>